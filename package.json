{
  "name": "jumpy_kart",
  "version": "1.0.0",
  "description": "MERN Stack project - mutiplayer jumping kart racer",
  "main": "app.js",
  "scripts": {
    "start": "node -r esm app.js",
    "server": "nodemon -r esm app.js",
    "server:debug": "nodemon -r esm --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FueRobertHer/jumpy_kart.git"
  },
  "author": "Andrew, Fue, Owen, Taehoon",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.2"
  },
  "bugs": {
    "url": "https://github.com/FueRobertHer/jumpy_kart/issues"
  },
  "homepage": "https://github.com/FueRobertHer/jumpy_kart#readme"
